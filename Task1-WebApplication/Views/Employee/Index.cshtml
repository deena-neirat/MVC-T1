
@model List<Employee>

    <h2>All Employees</h2>
    <table class="table">
        <thead>
            <tr>
                 <th scope="col">Id</th>
                 <th scope="col">Name</th>
                 <th scope="col">Age</th>
                 <th scope="col">Title</th>
            </tr>
            
        </thead>
        <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Title</td>
            </tr>
           
        }
        </tbody>
    </table>

